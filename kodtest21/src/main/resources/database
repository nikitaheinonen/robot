CREATE TABLE movies(
   Title      VARCHAR(49) NOT NULL PRIMARY KEY
  ,Year       INTEGER  NOT NULL
  ,Rated      VARCHAR(9) NOT NULL
  ,Released   DATE  NOT NULL
  ,Runtime    VARCHAR(7) NOT NULL
  ,Genre      VARCHAR(26) NOT NULL
  ,Director   VARCHAR(30) NOT NULL
  ,Writer     VARCHAR(203) NOT NULL
  ,Actors     VARCHAR(66) NOT NULL
  ,Plot       VARCHAR(238) NOT NULL
  ,Language   VARCHAR(38) NOT NULL
  ,Country    VARCHAR(31) NOT NULL
  ,Awards     VARCHAR(57) NOT NULL
  ,Poster     VARCHAR(166) NOT NULL
  ,Metascore  VARCHAR(3) NOT NULL
  ,imdbRating NUMERIC(3,1) NOT NULL
  ,imdbVotes  INTEGER  NOT NULL
  ,imdbID     VARCHAR(9) NOT NULL
  ,Type       VARCHAR(5) NOT NULL
  ,Response   VARCHAR(4) NOT NULL
);
